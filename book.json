{
  "gitbook": "3.2.2",
  "title": "The Openpilot Documentation Project",
  "description": "",
  "plugins": [
    "edit-link",
    "github",
    "alerts",
    "chapter-fold",
    "noembed",
    "last-modified",
    "download-pdf-link",
    "add-js-css",
    "itoc"
  ],
  "pluginsConfig": {
    "download-pdf-link": {
      "base" : "https://opc.ai/documentation/"
    },
    "edit-link": {
      "base": "https://github.com/openpilot-community/documentation/tree/master",
      "label": "Edit This Page"
    },
    "add-js-css": {
      "js": [
        "./docs/docs.js"
      ],
      "css": [
        "./docs/docs.css"
      ]
    },
    "github": {
      "url": "https://github.com/openpilot-community/documentation/"
    }
  }
}
